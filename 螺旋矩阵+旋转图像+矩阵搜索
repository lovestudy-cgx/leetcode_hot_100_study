class Solution(object):
    def spiralOrder(self, matrix):
        """
        :type matrix: List[List[int]]
        :rtype: List[int]
        """
        m=len(matrix)
        n=len(matrix[0])
        list1=[]
        #é€’å½’
        i,j=0,0
        while(i<m):
            if j==n:
                j=n-1
                while(j<n):
                    list1.append(matrix[i][j])
                    j+=1
            i+=1
        i-=1
        if m==n and m==1:
            return list1
        for j in range(n-1,-1,-1):
            list1.append(matrix[i][j])
        self.spiralOrder(matrix[1:m-1][0:])
ä¸€å¨ï¼Œé€»è¾‘é”™äº†ï¼šæˆ‘æƒ³å»æ‰3/4åœˆï¼Œä¸å¯¹

è¯„è®ºåŒºè¿˜æœ‰ç‰›çš„æ–¹ä¾¿ç†è§£çš„ï¼š
class Solution:
    def spiralOrder(self, matrix):
        """
        :type matrix: List[List[int]]
        :rtype: List[int]
        """
        ans = []
        m, n = len(matrix), len(matrix[0])
        left, right, top, bottom = 0, n - 1, 0, m - 1

        while left <= right and top <= bottom:
            # ä»å·¦åˆ°å³
            for i in range(left, right + 1):
                ans.append(matrix[top][i])
            top += 1

            # ä»ä¸Šåˆ°ä¸‹
            for i in range(top, bottom + 1):
                ans.append(matrix[i][right])
            right -= 1

            # ä»å³åˆ°å·¦
            if top <= bottom:
                for i in range(right, left - 1, -1):
                    ans.append(matrix[bottom][i])
                bottom -= 1

            # ä»ä¸‹åˆ°ä¸Š
            if left <= right:
                for i in range(bottom, top - 1, -1):
                    ans.append(matrix[i][left])
                left += 1

        return ans

å®˜ç½‘ç­”æ¡ˆï¼š
class Solution:
    def spiralOrder(self, matrix: List[List[int]]) -> List[int]:
        if not matrix or not matrix[0]:
            return list()
        
        rows, columns = len(matrix), len(matrix[0])
        visited = [[False] * columns for _ in range(rows)]
        total = rows * columns
        order = [0] * total

        directions = [[0, 1], [1, 0], [0, -1], [-1, 0]]
        row, column = 0, 0
        directionIndex = 0
        for i in range(total):
            order[i] = matrix[row][column]#å°†äºŒç»´è½¬åŒ–ä¸º1ç»´è®°å½•
            visited[row][column] = True#æ ‡è®°ï¼ŒæŠŠç»è¿‡çš„å€¼ä»Fæ”¹æˆT
            nextRow, nextColumn = row + directions[directionIndex][0], column + directions[directionIndex][1]#å­åˆ—è¡¨ä¸­ç¬¬ä¸€ä¸ªæ˜¯è¡Œï¼Œç¬¬äºŒä¸ªæ˜¯åˆ—ï¼Œæ‰€ä»¥ç¬¬ä¸€æ¬¡å¾ªç¯ç›¸å½“äºåˆ—åŠ 1
            if not (0 <= nextRow < rows and 0 <= nextColumn < columns and not visited[nextRow][nextColumn]):#è¿™ä¹ˆå¤šæ¡ä»¶çš„å¯¹ç«‹ï¼šå¾·æ‘©æ ¹ç‡å°±æ˜¯è¿™äº›æ¡ä»¶ä¸æ»¡è¶³çš„å¹¶é›†ï¼ˆor)ï¼›not visited[nextRow][nextColumn] æ˜¯ä»€ä¹ˆæ„æ€ï¼Ÿ
#è¿™ä¸€å¥é€»è¾‘ï¼šå¦‚æœä¸‹ä¸€ä¸ªä½ç½®è¿˜æ²¡æœ‰è¢«è®¿é—®è¿‡ï¼ˆä¹Ÿå°±æ˜¯ Falseï¼‰ï¼Œé‚£å°±å¯ä»¥ç»§ç»­å¾€é‚£ä¸ªæ–¹å‘èµ°
                directionIndex = (directionIndex + 1) % 4#å› ä¸ºå¾ˆå¯èƒ½ä¸æ­¢ä¸€åœˆ
            row += directions[directionIndex][0]#ç¬¬äºŒè¡Œé‡Œé¢çš„å…ƒç´ ï¼ŒåªåŠ è¡Œï¼Œç¬¬ä¸€è¡Œæ˜¯åªåŠ åˆ—ï¼Œç¬¬ä¸‰è¡Œæ˜¯åªå‡åˆ—ï¼Œç¬¬å››æ¬¡åªå‡è¡Œ
            column += directions[directionIndex][1]
        return order



æ—‹è½¬å›¾åƒï¼ˆåŸæœ‰çŸ©é˜µè½¬90åº¦ï¼‰ï¼š
å®˜æ–¹è§£æ³•ï¼ˆéš¾ç‚¹åœ¨äºè¦æ±‚åŸåœ°æ“ä½œï¼Œç©ºé—´o1
class Solution:
    def rotate(self, matrix: List[List[int]]) -> None:
        n = len(matrix)
        for i in range(n // 2):
            for j in range((n + 1) // 2):
                matrix[i][j], matrix[n - j - 1][i], matrix[n - i - 1][n - j - 1], matrix[j][n - i - 1] \
                    = matrix[n - j - 1][i], matrix[n - i - 1][n - j - 1], matrix[j][n - i - 1], matrix[i][j]
ç¿»è½¬å¼è§£æ³•ï¼š
class Solution:
    def rotate(self, matrix: List[List[int]]) -> None:
        n = len(matrix)
        # æ°´å¹³ç¿»è½¬
        for i in range(n // 2):
            for j in range(n):
                matrix[i][j], matrix[n - i - 1][j] = matrix[n - i - 1][j], matrix[i][j]
        # ä¸»å¯¹è§’çº¿ç¿»è½¬
        for i in range(n):
            for j in range(i):
                matrix[i][j], matrix[j][i] = matrix[j][i], matrix[i][j]

å¦‚æœä¸è€ƒè™‘ç©ºé—´ï¼Œé‚£ä¹ˆ
class Solution:
    def rotate(self, matrix: List[List[int]]) -> None:
        n = len(matrix)
        # Python è¿™é‡Œä¸èƒ½ matrix_new = matrix æˆ– matrix_new = matrix[:] å› ä¸ºæ˜¯å¼•ç”¨æ‹·è´ï¼ˆè½¯æ‹·è´ï¼‰
        matrix_new = [[0] * n for _ in range(n)]
        for i in range(n):
            for j in range(n):
                matrix_new[j][n - i - 1] = matrix[i][j]
        # ä¸èƒ½å†™æˆ matrix = matrix_new
        matrix[:] = matrix_new

